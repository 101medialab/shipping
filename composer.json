{
    "name":        "101medialab/shipping",
    "type":        "library",
    "description": "Shipping cost estimation library.",
    "license":     "MIT",
    "homepage":    "http://101medialab.com",
    "authors": [
        {
            "name":     "101 Media Lab",
            "homepage": "http://101medialab.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/JeremyDunn/php-fedex-api-wrapper"
        }
    ],
    "require": {
        "php": ">=5.5",
        "sylius/shipping": "~0.11",
        "sylius/addressing": "~0.11",
        "jeremy-dunn/php-fedex-api-wrapper": "dev-master",
        "icanboogie/inflector": "1.2",
        "guzzle/http": "~3.9"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0",
        "fzaninotto/faker": "~1.5",
        "sylius/core": "~0.11"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "MediaLab\\Shipping\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev"
}
